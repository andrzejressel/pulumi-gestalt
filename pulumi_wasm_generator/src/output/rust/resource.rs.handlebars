    fn {{interface.function_name}}<'a>(args: {{interface.struct_name}}Args<'a>) -> {{interface.struct_name}}Result {

        let result = crate::bindings::pulumi::{{@root.package.name}}::{{interface.wit_name}}::invoke(args.name, &crate::bindings::pulumi::{{@root.package.name}}::{{interface.wit_name}}::Args {
    {{#each interface.input_properties}}
            {{wit_name}}: &crate::clone::<{{&type_}}>(args.{{name}}),
    {{/each}}
        });


        let result = {{interface.struct_name}}Result {
        {{#each interface.output_properties}}
            {{name}}: crate::random_to_domain_mapper::<{{&type_}}>(result.{{wit_name}}),
        {{/each}}
        };

        result
    }

}

