{{+ resources|safe }}

pub mod functions {
    {{+ functions|safe }}
}

pub mod types {
    {{+ types|safe }}
}

#[doc(hidden)]
pub mod constants {
{% for constant in constants +%}
    pulumi_wasm_rust::__private::constant::generate_string_const!(ConstString{{ constant }}, "{{ constant }}");
{% endfor %}
}

mod bindings {
    pulumi_wasm_rust::__private::wit_bindgen::generate!({
        // the name of the world in the `*.wit` input file
        inline: r"
            {{ pulumi_wasm_wit|safe }}
        ",
        with: {
            "component:pulumi-wasm/output-interface@{{pulumi_wasm_version}}": pulumi_wasm_rust::__private::pulumi_wasm_wit::client_bindings::component::pulumi_wasm::output_interface
        }
    });
}

#[link_section = "pulumi_wasm_provider::{{ provider_name }}"]
#[no_mangle]
pub static PULUMI_WASM_PROVIDER_{{ provider_name.replace("-", "_") }}: [u8; {{ provider_version.len() }}] = *b"{{ provider_version }}";
