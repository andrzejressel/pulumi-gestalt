    description="Athena KMS Key")
test_workgroup = aws.athena.Workgroup("test",
    name="example",
    configuration={
        "result_configuration": {
            "encryption_configuration": {
                "encryption_option": "SSE_KMS",
                "kms_key_arn": test.arn,
            },
